    "version": "0.2.0",
    "configurations": [
        {
            "name": "scanhistory pingslurp",
            "type": "python",
            "request": "launch",
            "envFile": "${workspaceFolder}/.env",
            "program": "${workspaceFolder}/src/pingslurp.py",
            "args": [
                "scanhistory",
                "--start-days",
                "5",
                "--bots",
                "20"
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
        },
